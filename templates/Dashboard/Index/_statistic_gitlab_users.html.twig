{% if gitlabUsersFailed is defined and gitlabUsersFailed == false  %}
<div class="col-md-6">
    <!-- USERS LIST -->
    <div class="card">
        <div class="card-header">
            <h3 class="card-title">Gitlab-Mitglieder</h3>

            <div class="card-tools">
                <a href="{{ path('gitlab_users') }}" class="badge badge-primary">Alle Benutzer anzeigen</a>
                <button type="button" class="btn btn-tool" data-card-widget="collapse">
                    <i class="fas fa-minus"></i>
                </button>
                <button type="button" class="btn btn-tool" data-card-widget="remove">
                    <i class="fas fa-times"></i>
                </button>
            </div>
        </div>
        <!-- /.card-header -->
        <div class="card-body p-0">
            <ul class="users-list clearfix">
                {% for user in gitlabUsers %}
                    <li>
                        <img src="{{ user.avatar_url }}" alt="User Image" style="width: 80px;height: 80px">
                        <span class="users-list-name" >{{ user.username }}</span>
                        <span class="users-list-date">{{ user.email }}</span>
                    </li>
                {% endfor %}

            </ul>
            <!-- /.users-list -->
        </div>
    </div>
    <!--/.card -->
</div>
{% endif %}