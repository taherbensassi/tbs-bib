{% extends 'base.html.twig' %}
{% block title %}Export Module{% endblock %}
{% block stylesheets %}
    {{ parent() }}
    <link rel="stylesheet" href="{{ asset('plugins/bootstrap4-duallistbox/bootstrap-duallistbox.min.css') }}">
{% endblock %}
{% block javascripts %}
    {{ parent() }}
    <script src="{{ asset('plugins/bootstrap4-duallistbox/jquery.bootstrap-duallistbox.min.js') }}" ></script>
    <script src="{{ asset('dist/js/module/export-content-element.js') }}" ></script>
{% endblock %}
{% block content %}
    <div class="content-wrapper">
        {% include 'Partial/Breadcrumb/_breadcrumb.htnl.twig' with {'breadcrumb': 'Export Module'} %}
        <div class="content">
            <div class="container-fluid">
                <div class="card border-0">

                    <div class="card-header  d-flex justify-content-between align-items-right">
                        <h3 class="card-title">Export Module</h3>
                        <a href="{{ path('export_content_element_index') }}" class="btn btn-outline-dark  ml-auto">
                            Abbrechen
                        </a>
                    </div>

                    {{ include('Dashboard/Content Elements/Export/_form.html.twig') }}

                </div>
            </div>
        </div>
    </div>
{% endblock %}
